alias pylab="ipython --pylab"
alias py='ipython'
alias cls='ls --color=auto'
alias tmux="tmux -2"
alias valgrind="valgrind --log-file=valgrind.log --leak-check=full"
alias wasphead="ssh -t sw@wasphead.star.le.ac.uk 'source ~/.zshrc; tmux attach-session -t PROGRESS'"
alias g=git

# Set some common typos
alias sl=ls
alias csl=cls

# Vim no longer uses X
alias vim='vim -X'
alias view='view -X'

# Stop using grep 
alias grep=ack

