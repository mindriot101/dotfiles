alias pylab="ipython --pylab"
alias py=pylab
alias cls='ls --color=auto'
alias g=git

# Set some common typos
alias sl=ls
alias csl=cls
alias lcs=cls

# Sometimes pydoc does not work with virtualenvs
alias pydoc='python -m pydoc'

# Some suffix aliases
alias -s rb=vim
alias -s py=vim
alias -s cpp=vim
alias -s c=vim
