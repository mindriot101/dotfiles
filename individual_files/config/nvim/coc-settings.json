{
    "python.jediEnabled": true,
    "python.linting.enabled": false,
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"]
		}
	},
    "coc.preferences.formatOnSaveFiletypes": [
        "rust"
    ],
    "rust-analyzer.enableCargoWatchOnStartup": "disabled",
    "suggest.autoTrigger": "trigger",
    "diagnostic.enable": false
}
