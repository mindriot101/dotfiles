--langdef=latex
--langmap=latex:.tex
--regex-latex=/\s*\\label\{([^}]*)\}/\1/l,label/
--regex-latex=/\s*\\section\{([^}]*)\}/\1/s,section/
--regex-latex=/\s*\\subsection\{([^}]*)\}/\1/t,subsection/
--regex-latex=/\s*\\subsubsection\{([^}]*)\}/\1/u,subsubsection/
--regex-latex=/\s*\\section\*\{([^}]*)\}/\1/s,section/
--regex-latex=/\s*\\subsection\*\{([^}]*)\}/\1/t,subsection/
--regex-latex=/\s*\\subsubsection\*\{([^}]*)\}/\1/u,subsubsection/
